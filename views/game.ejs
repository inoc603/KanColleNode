<html>
	<body>
		<!-- "http://www.dmm.com/netgame/social/-/gadgets/=/app_id=854854/" -->
		<div id="game_div" style="width:800px;height:480px;overflow:hidden;border-style:solid;border-width:2px">
			<iframe 
				id="game_frame" 
				name="game_frame" 
				width="900" 
				height="1200" 
				scrolling="no" 
				frameborder="1" 
				border="0" 
				style="positon: relative;
					margin: -78px 0px 0px -71px;
					border-style: solid;
					border-width: 1px;
					border-color: red"
				>
			</iframe>
		</div>
		<a href="/index">Back to Index</a>
		<div id="admaralInfo">
			<p>Name: {{nickname}}</p>
			<p>lv {{level}}. Rank:{{rank}}</p>
		</div>
		<button id="load_game">Load</button>
		<button id="unload_game">Unload</button>
		
	</body>
	<script src="/javascripts/jquery-2.1.3.min.js"></script>
	<script>
		$("#load_game").click(function () {
			$("#game_frame").attr("src", "http://www.dmm.com/netgame/social/-/gadgets/=/app_id=854854/");
		});
		$("#unload_game").click(function () {
			$("#game_frame").removeAttr("src");
		});

	</script>
</html>